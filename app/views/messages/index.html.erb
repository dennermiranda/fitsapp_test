<p id="notice"><%= notice %></p>

<h1>Sent messages</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sent_messages.each do |sent| %>
      <tr>
        <td><%= sent.question %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message', newmessage_path %>
